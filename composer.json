{
    "name": "talis/babel-php-client",
    "description": "This is a PHP client library for Babel.",
    "keywords": ["php"],
    "homepage": "https://github.com/talis/babel-php-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":"Andrew Bate",
            "email": "abs@talis.com",
            "homepage":"http://engineering.talis.com"
        }
